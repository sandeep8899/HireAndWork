<table border="1">
	<tr>
		<th colspan="4"> All User Available </th>
	</tr>
	<tr>
		<th> Name </th>
		<th> Skills </th>
		<th> Experience </th>
		<th> View </th>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td> <%= user.name %> </td>
			<td> <%= user.skills %> </td>
			<td> <%= user.experience %> </td>
			<td> <%= link_to "view full details" , user_detail_path(user) %> </td>
		</tr>
	<% end %>
</table>